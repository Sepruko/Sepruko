{
	"$schema": "https://raw.githubusercontent.com/streetsidesoftware/cspell/main/cspell.schema.json",
	"version": "0.2",
	"name": "seprukoProfile",
	"language": "en_US",
	"dictionaries": [
		"creativeCommons",
		"filetypes",
		"node",
		"npm",
		"profile",
		"softwareTerms",
		"typescript",
	],
	"dictionaryDefinitions": [
		{
			"name": "profile",
			"path": "./.cspell/profile.txt",
			"addWords": true
		},
		{
			"name": "creativeCommons",
			"path": "./.cspell/cc.txt",
			"addWords": true
		}
	],
	"ignorePaths": ["**/node_modules/", "pnpm-lock.yaml"],
	"minWordLength": 4
}
